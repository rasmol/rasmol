xx = -lXpm -L/sw/lib -lforms
GUIOPTION = -DXFORMSLIB -I/sw/include

        PROGRAM = rasmol

all:: rasmol

rasmol: $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) $(OBJS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: rasmol
	@if [ -d $(DESTDIR)$(BINDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(BINDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(BINDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  rasmol $(DESTDIR)$(BINDIR)/rasmol

all:: rasmol.$(MANSUFFIX).html

rasmol.$(MANSUFFIX).html: rasmol.$(MANNEWSUFFIX)
	$(RM) rasmol.$(MANSUFFIX).html rasmol.$(MANSUFFIX)-html
	$(RMAN) $(RMANOPTIONS) < rasmol.$(MANNEWSUFFIX) \
	  > rasmol.$(MANSUFFIX)-html && $(MV) rasmol.$(MANSUFFIX)-html $@

install.man:: rasmol.$(MANSUFFIX).html
	@if [ -d $(DESTDIR)$(DOCHTMLDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(DOCHTMLDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(DOCHTMLDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(DOCHTMLDIR)); \
	fi
	@(SUF=`expr $(MANSUFFIX) \: '\(.\)'`; \
	 set -x; \
	 $(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) rasmol.$(MANSUFFIX).html $(DESTDIR)$(DOCHTMLDIR)/rasmol.$$SUF.html)

cleandir::
	$(RM) rasmol.$(MANSUFFIX).html rasmol.$(MANSUFFIX)-html

all:: rasmol.$(MANNEWSUFFIX)

rasmol.$(MANNEWSUFFIX): rasmol.$(MANSRCSUFFIX)
	$(RM) $@
	cd `dirname rasmol` && \
		$(LN) `basename rasmol.$(MANSRCSUFFIX)` `basename $@`

cleandir::
	$(RM) rasmol.$(MANNEWSUFFIX)

install.man:: rasmol.$(MANNEWSUFFIX)
	@if [ -d $(DESTDIR)$(MANDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(MANDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(MANDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) rasmol.$(MANNEWSUFFIX) $(DESTDIR)$(MANDIR)/rasmol.$(MANSUFFIX)

depend::
	$(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

cleandir::
	$(RM) rasmol

install::
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[i]*) set +e;; esac; done; \
	for i in $(RASMOLDIR); do if [ -d $(DESTDIR)$$i ]; then \
		set +x; else (set -x; $(MKDIRHIER) $(DESTDIR)$$i); fi; \
	done

install:: ../doc/rasmol.hlp
	$(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) ../doc/rasmol.hlp $(DESTDIR)$(RASMOLDIR)

install:: ../data/1crn.pdb
	$(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) ../data/1crn.pdb $(DESTDIR)$(RASMOLDIR)

install:: ../data/3cro.pdb
	$(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) ../data/3cro.pdb $(DESTDIR)$(RASMOLDIR)

cleandir::
	   $(RM) gtkui.h rasmol.man

rasmol.man::	../doc/rasmol.1
		ln -s ../doc/rasmol.1 rasmol.man

../doc/rasmol.1:	../doc/rasmol.1.gz
		gunzip  <../doc/rasmol.1.gz > ../doc/rasmol.1

$(CBFLIBDIR):
		(cd $(CBFLIB_TARBALLDIR);		  wget $(CBFLIB_TARBALL_URL);		  gunzip < $(CBFLIB_TARBALL) | tar xvf -)

$(CBFLIBDIR)/lib/libcbf.a: $(CBFLIBDIR)
		(cd $(CBFLIBDIR); make lib/libcbf.a)

$(OBJS):  $(CBFLIBDIR)/lib/libcbf.a

gtkwin.c: gtkui.h

gtkui.h: sizechooser.glade printing-resolution.glade actionmenu.gtk
	 echo "/* Automatically generated GTK ui definitions, do not edit! */" > gtkui.h
	 echo >> gtkui.h
	 echo "const gchar *actionmenu_str = \"\\" >> gtkui.h
	 cat actionmenu.gtk | sed "s/$$/\\\/" | sed 's/"/\\\"/g' >> gtkui.h
	 echo "\";" >> gtkui.h
	 echo >> gtkui.h
	 echo "const gchar *sizechooser_str = \"\\" >> gtkui.h
	 gtk-builder-convert -w sizechooser.glade - | sed "s/$$/\\\/" | sed 's/"/\\\"/g' >> gtkui.h
	 echo "\";" >> gtkui.h
	 echo >> gtkui.h
	 echo "const gchar *print_resolution_str = \"\\" >> gtkui.h
	 gtk-builder-convert -w printing-resolution.glade - | sed "s/$$/\\\/" | sed 's/"/\\\"/g' >> gtkui.h
	 echo "\";" >> gtkui.h
	 echo >> gtkui.h

# ----------------------------------------------------------------------
# common rules for all Makefiles - do not edit

.c.i:
	$(RM) $@
	 	$(CC) -E $(CFLAGS) $(_NOOP_) $*.c > $@

.SUFFIXES: .s

.c.s:
	$(RM) $@
	 	$(CC) -S $(CFLAGS) $(_NOOP_) $*.c

emptyrule::

cleandir::
	$(RM) *.CKP *.ln *.BAK *.bak *.o core errs ,* *~ *.a .emacs_* tags TAGS make.log MakeOut  "#"*

Makefile::
	-@if [ -f Makefile ]; then set -x; \
	$(RM) Makefile.bak; $(MV) Makefile Makefile.bak; \
	else exit 0; fi
	$(IMAKE_CMD) -DTOPDIR=$(TOP) -DCURDIR=$(CURRENT_DIR)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

man_keywords::

html_index::
	$(HTMLINDEXCMD) $(DESTDIR)$(DOCHTMLDIR)

clean:: cleandir

distclean:: cleandir

# ----------------------------------------------------------------------
