		(cd $(CQRLIB_TARBALLDIR);		  wget $(CQRLIB_TARBALL_URL);		  touch $(CQRLIB_TARBALL);		  gunzip < $(CQRLIB_TARBALL) | tar xvf -)

$(CQRLIB_DIR)/cqrlib.c: $(CQRLIB_DIR)

