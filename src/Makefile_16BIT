#
#    If running under HPUX, define the appropriate variant
#    with #define
#

        PROGRAM = rasmol

all:: rasmol

rasmol: $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) $(OBJS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: rasmol
	@if [ -d $(DESTDIR)$(BINDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(BINDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(BINDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  rasmol $(DESTDIR)$(BINDIR)/rasmol

all:: rasmol.$(MANNEWSUFFIX)

rasmol.$(MANNEWSUFFIX): rasmol.$(MANSRCSUFFIX)
	$(RM) $@
	if test -z "$(PREPROCESSMANPAGES)" ; then \
	   cd `dirname rasmol` && \
	   $(LN) `basename rasmol.$(MANSRCSUFFIX)` `basename $@`; \
	else \
	      	     $(RAWCPP)  $(MANDEFS) $(EXTRAMANDEFS) \
	     < rasmol.$(MANSRCSUFFIX) | sed -e '/^#  *[0-9][0-9]*  *.*$$/d' 			-e '/^#line  *[0-9][0-9]*  *.*$$/d' 			-e '/^[ 	]*XCOMM$$/s/XCOMM/#/' 			-e '/^[ 	]*XCOMM[^a-zA-Z0-9_]/s/XCOMM/#/' 			-e '/^[ 	]*XHASH/s/XHASH/#/' 			-e '/\@\@$$/s/\@\@$$/\\/' >$@; \
	fi

cleandir::
	$(RM) rasmol.$(MANNEWSUFFIX)

install.man:: rasmol.$(MANNEWSUFFIX)
	@if [ -d $(DESTDIR)$(MANDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(MANDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(MANDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) rasmol.$(MANNEWSUFFIX) $(DESTDIR)$(MANDIR)/rasmol.$(MANSUFFIX)

depend::
	$(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

cleandir::
	$(RM) rasmol

install::
	@for flag in ${MAKEFLAGS} ""; do \
	  case "$$flag" in *=*) ;; --*) ;; *[i]*) set +e;; esac; done; \
	for i in $(RASMOLDIR); do if [ -d $(DESTDIR)$$i ]; then \
		set +x; else (set -x; $(MKDIRHIER) $(DESTDIR)$$i); fi; \
	done

install:: ../doc/rasmol.hlp
	$(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) ../doc/rasmol.hlp $(DESTDIR)$(RASMOLDIR)

install:: ../data/1crn.pdb
	$(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) ../data/1crn.pdb $(DESTDIR)$(RASMOLDIR)

install:: ../data/3cro.pdb
	$(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) ../data/3cro.pdb $(DESTDIR)$(RASMOLDIR)

cleandir::
	   $(RM) gtkui.h rasmol.man

rasmol.man::	../doc/rasmol.1
		ln -s ../doc/rasmol.1 rasmol.man

../doc/rasmol.1:	../doc/rasmol.1.gz
		gunzip  <../doc/rasmol.1.gz > ../doc/rasmol.1

$(EXTPKGDIR):
		mkdir -p -v $(EXTPKGDIR)

$(XFORMSLIB_DIR):
		(cd $(XFORMSLIB_TARBALLDIR);		  wget $(XFORMSLIB_TARBALL_URL);		  touch $(XFORMSLIB_TARBALL);		  gunzip < $(XFORMSLIB_TARBALL) | tar xvf -)

$(XFORMSLIB_DIR)/lib/.libs/libforms.a: $(XFORMSLIB_DIR)
		(cd $(XFORMSLIB_DIR); ./configure --enable-static)
		(cd $(XFORMSLIB_DIR)/lib; make all)

